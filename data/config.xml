<odoo>
	<record model="ir.ui.view" id="view_settings_my_business_url">
		<field name="name">res.config.settings.form.inherit</field>
		<field name="model">res.config.settings</field>
		<field name="inherit_id" ref="base.res_config_settings_view_form"/>
		<field name="arch" type="xml">
			<xpath expr="//div[hasclass('o_settings_container')]" position="after">
				<div id="my_business_url">
					<h2>My Business</h2>
					<div class="row mt16 o_settings_container" id="my_business_url_setting">
						<div class="col-xs-12 col-md-6 o_setting_box">
							<div class="o_setting_right_pane">
								<div class="w-50">
									<h3>Url MyBusiness</h3>
									<field name="mnmb_mybusiness_url" placeholder="My Business URL"
									       class="o_input" style="width: 100%;"/>
								</div>
							</div>
						</div>
					</div>
				</div>
			</xpath>
		</field>
	</record>
</odoo>